#!/usr/bin/env bash
# Displays number 1 - 20 with variations for 4th, 9th, and 17th iterations
x=1
while [ $x -le 10 ]
do
        echo "$x"
        x=$((x + 1))
	if [[ "$x" == '4' ]];
	then
		echo "bad luck from China"
	elif [[ "$x" == '9' ]];
	then
		echo "bad luck from Japan"
	elif [[ "$x" == '17' ]];
	then
		echo "bad luck from Italy"
	fi
		continue
done
